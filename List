#### list

my_list=[1,2,3,4]
print(my_list)   ##(*my_list) if wee using this brackets remove
#methods in lst
#1.append
my_list.append(5)
#2.extend
my_list.extend([6,7,8])
#3.insert
my_list.insert(7,11)
#4.remove
my_list.remove(5)
#5.pop()
my_list.pop()  # pop is remove the last element
#6.clear()
my_list.clear()
#7.count()
my_list.count()
#8.copy
my_list.copy()
